{
    "name": "greensight/laravel-elastic-query-specification",
    "description": "",
    "keywords": [
        "Greensight",
        "laravel",
        "laravel-elastic-query-specification"
    ],
    "homepage": "https://github.com/greensight/laravel-elastic-query-specification",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Krol",
            "email": "crol@greensight.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "elasticsearch/elasticsearch": "^7.13",
        "greensight/laravel-elastic-query": "^0.2.0",
        "illuminate/contracts": "^8.37",
        "illuminate/support": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "php-parallel-lint/php-var-dump-check": "^0.5.0",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Greensight\\LaravelElasticQuerySpecification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greensight\\LaravelElasticQuerySpecification\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --config .php_cs",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElasticQuerySpecificationServiceProvider"
            ]
        }
    }
}
